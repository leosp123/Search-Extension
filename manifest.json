{
    "manifest_version": 2,
    "name": "SBU Classie Evals quick access",
    "version": "1.00",
    "content_scripts": 
    [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": 
    {
        "default_popup": "extension.html",
        "default_title": "popup"
    }
}